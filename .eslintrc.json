{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "brace-style": "off",
        "camelcase": "off",
        "linebreak-style": ["error", "windows"]
    }
}
